    "eslint-plugin-jsdoc": ">= 62",
    "eslint-plugin-sort-class-members": ">= 1.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@types/node": "^25.0.10",
    "eslint": "^9.39.2",
    "eslint-plugin-jsdoc": "^62.4.1",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "typedoc": "^0.28.3",
    "typescript": "~5.7.2"
  }
}
